<h2>User information</h2>
  <p>投稿ユーザー画像：<%= image_tag bookers.user.get_profile_image(100,100) %></p>
<h2>New book</h2>
  <%= form_with model: @book do |f| %>
    <h4>Image</h4>
      <%= f.file_field :image, accept: "image/*" %>
    <h4>Title</h4>
      <%= f.text_field :shop_name %>
    <h4>Opinion</h4>
      <%= f.text_area :caption %>
    <%= link_to 'New Book', new_book_path, class: "btn btn-secondary" %>
  <% end %>